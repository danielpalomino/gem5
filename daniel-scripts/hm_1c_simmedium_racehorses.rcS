#!/bin/sh

# File to run the x264 benchmark

cd /hm/bin

#step 100.000.000 which means 1K dumps per second
/sbin/m5 dumpstats 0 1000000000
/sbin/m5 resetstats
echo "Starting..."
./TAppEncoderStatic_ALPHA -c /hm/cfg/encoder_lowdelay_P_main_2Ref.cfg -c /hm/cfg/per-sequence/RaceHorsesC.cfg -f 10 --InputFile=../../origCfP/RaceHorses_30frames.yuv --QP=32 --AC_DataLevel=0 --AC_AlgorithmLevel=0 --HadamardME=0 --FEN=0 --FDM=0 --TimePerFrame=0
echo "Done :D"
/sbin/m5 exit
/sbin/m5 exit